<Window x:Class="PetShelter.View.FiltreWindows.Vaccine_Filtre"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PetShelter.View.FiltreWindows"
        mc:Ignorable="d"
        Title="Фільтраціія" Height="400" Width="350">
    <Grid Background="#FFE5DDD3">
        <ScrollViewer>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="5*"/>
                    <RowDefinition Height="1*"/>
                </Grid.RowDefinitions>
                <Label Name="ColumnLabel1"
                   Content="Виробник"
                   FontSize="20"
                   VerticalAlignment="Center"
                   HorizontalAlignment="Center"></Label>
                <StackPanel Name="StackPanel1"
                        Grid.Row="1"
                        Grid.Column="0"></StackPanel>
                <Button Name="FiltreButton"
                    Grid.ColumnSpan="4"
                    Grid.Row="2"
                    Width="200"
                    Height="40"
                    Click="FiltreButton_Click">Фільтрувати</Button>
            </Grid>
        </ScrollViewer>
    </Grid>
</Window>
